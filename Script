INSERT INTO website_role_feature_menu (id, role, website_details_id, feature_menu_id)
VALUES (NEXTVAL('website_role_feature_menu_seq'), 'ROLE_ADMIN', (select key from website_details where web_site_type ='ECOMMERCE_WEBSITE'), (select key from feature_menu where feature_name ='Inquiry'));


--join
select wfm.key,wfm.is_active,feature_name,wd.web_site_type
from website_feature_menu wfm,feature_menu fm,website_details wd
where fm.key=wfm.feature_menu_key and wfm.website_details_key=wd.key and wd.web_site_type='ECOMMERCE_WEBSITE'
order by wfm.key;
